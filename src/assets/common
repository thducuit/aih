        isChange = !0;
        var e = $(this).attr("tab");
        $(this).hasClass("active") || ($(".ct-tab span").removeClass("active"), $(this).addClass("active"), $(".tab-content").hide(), $("#" + e).fadeIn(function() {
            isChange = !1
        }))
    }
    return !1
}


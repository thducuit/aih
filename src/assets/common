    return closePopup(), $(".ovl").fadeIn(function() {
        $("#" + e).fadeIn()
    }), $("html,body").css({
        overflow: "hidden"
    }), !1
}

function closePopup() {
    return $(".ovl,.popup").fadeOut(), $("html,body").css({
        overflow: "auto"
    }), !1
}

